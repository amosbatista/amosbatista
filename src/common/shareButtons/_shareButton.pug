.share-container

	span.share-item.share-title Compartilhe:

	a.share-item.share-facebook(href="" ng-click="fbShare()")
		span.logo
			i.fa.fa-facebook

	a.share-item.share-twitter(href="" ng-href="{{twitterShareLink}}" target="_blank")
		span.logo
			i.fa.fa-twitter

	a.share-item.share-linkedin(href="" ng-href="{{linkedinShareLink}}" target="_blank")
		span.logo
			i.fa.fa-linkedin